package dam.psp;


public class Hilo extends Thread{
	private int _ID;
	private Acceso access;
	Hilo(int id,Acceso access)
	{
		this._ID=id;
		this.access=access;
	}
	@Override
	public void run() {
				mensajeSend();
				
	}
	private void mensajeSend()
	{
		System.out.println("Hola, soy el hilo número "+_ID);
	}


	
}
