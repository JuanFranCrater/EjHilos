package dam.psp;

import java.util.concurrent.Semaphore;

public class Orden {
	private static Semaphore sem = new Semaphore(1);
	public static void main(String[] args) {
		
		Hilo hil1 = new Hilo(1,sem);
		Hilo hil2 = new Hilo(2,sem);
		
		hil2.start();
		
		hil1.start();
		
		
		try {
			hil1.join();
			hil2.join();
		} catch (InterruptedException e) {
			e.printStackTrace();
		}
	}
	
}