package dam.psp;


public class Hilo extends Thread{
	private int _ID;

	Hilo(int id)
	{
		this._ID=id;
	}
	@Override
	public void run() {
			while()
			{
			try {
				this.wait();
			} catch (InterruptedException e) {
				// TODO Auto-generated catch block
				e.printStackTrace();
			}
			}
				mensajeSend();
	}
	private void mensajeSend()
	{
		System.out.println("Hola, soy el hilo número "+_ID);
	}


	
}
